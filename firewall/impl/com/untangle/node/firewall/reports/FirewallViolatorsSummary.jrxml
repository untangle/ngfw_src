    <parameter name="startTime" isForPrompting="true" class="java.sql.Timestamp"/>
    <parameter name="endTime" isForPrompting="true" class="java.sql.Timestamp"/>
    <queryString><![CDATA[
SELECT c_client_addr AS HOST,
       COUNT(CASE proto WHEN 6  THEN 1 ELSE NULL END) AS TCP_BLOCKS,
       COUNT(CASE proto WHEN 17 THEN 1 ELSE NULL END) AS UDP_BLOCKS,
       COUNT(CASE proto WHEN 1  THEN 1 ELSE NULL END) AS PING_BLOCKS,
       COUNT(CASE proto WHEN 1 THEN 1 WHEN 6 THEN 1 WHEN 17 THEN 1 ELSE NULL END) AS TOTAL_BLOCKS
  FROM n_firewall_evt evt
         JOIN pl_endp endp ON evt.pl_endp_id = endp.event_id
  WHERE evt.time_stamp >= $P{startTime} AND evt.time_stamp < $P{endTime} AND evt.was_blocked
  GROUP BY HOST
  ORDER BY TOTAL_BLOCKS DESC, HOST ASC
  LIMIT 15
         ]]></queryString>
    <field name="HOST" class="java.lang.String"/>
    <field name="TCP_BLOCKS" class="java.lang.Long"/>
